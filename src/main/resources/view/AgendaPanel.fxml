<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TreeTableColumn?>
<?import javafx.scene.control.TreeTableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seedu.jimi.ui.AgendaPanel">
   <children>
      <AnchorPane prefHeight="200.0" prefWidth="200.0">
         <children>
            <TreeTableView fx:id="tasksTreeTableView" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <columns>
                  <TreeTableColumn fx:id="tasksTableColumnLeft" prefWidth="101.0" text="C1" />
                  <TreeTableColumn fx:id="tasksTableColumnRight" prefWidth="97.0" text="C2" />
               </columns>
               <columnResizePolicy>
                  <TreeTableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TreeTableView>
         </children>
      </AnchorPane>
      <AnchorPane prefHeight="200.0" prefWidth="200.0">
         <children>
            <TreeTableView fx:id="eventsTreeTableView" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <columns>
                  <TreeTableColumn fx:id="eventsTableColumnLeft" prefWidth="75.0" text="C1" />
                  <TreeTableColumn fx:id="eventsTableColumnRight" prefWidth="75.0" text="C2" />
               </columns>
               <columnResizePolicy>
                  <TreeTableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TreeTableView>
         </children>
      </AnchorPane>
   </children>
</VBox>
